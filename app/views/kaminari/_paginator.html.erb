<%= paginator.render do %>
  <nav>
    <ul class="pagination pagination-sm justify-content-end">
      <%= current_page.first? ? first_page_tag : prev_page_tag %>
      <% each_page do |page| %>
        <% if page.left_outer? || page.right_outer? || page.inside_window? %>
          <%= page_tag page %>
        <% elsif !page.was_truncated? -%>
          <%= gap_tag %>
        <% end %>
      <% end %>
      <%= current_page.last? ? last_page_tag : next_page_tag %>
    </ul>
  </nav>
<% end %>
